{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://lobarr.github.io",
  "dependencies": {
    "antd": "^3.6.1",
    "gh-pages": "^1.2.0",
    "immutable": "^3.8.2",
    "jquery": "^3.3.1",
    "motion-ui": "^2.0.0",
    "node-sass-chokidar": "^1.3.4",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-scripts": "^2.1.5",
    "react-scroll": "^1.7.9"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -b master -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "client": "npm-run-all -p start watch-css"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
